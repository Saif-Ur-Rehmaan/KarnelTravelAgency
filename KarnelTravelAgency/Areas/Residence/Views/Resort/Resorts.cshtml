<main class="min-vw-100">
    <div class="lp-bui-section-hotel-index bui-spacer--largest">
        <div class="lp-hero lp-hero--cover">
            <div class="lp-hero__content">
                <div class="lp-hero__bg-image " style="background-image: url('https://cf.bstatic.com');">
                    <div class="lp-hero__mask lp-hero__mask--campaign "></div>
                </div>
                <div class="lp-hero__searchbox-container ">
                    <div class="lp-hero__searchbox xpi__searchbox " id="indexsearch">
                        <h1 class="sb-searchbox__title">
                            <span class="sb-searchbox__title-text">
                                Discover Your Dream Resort with Karnel Travel Agency
                            </span>
                        </h1>
                        <h2 class="sb-searchbox__subtitle-text">
                            From budget hotels to luxury rooms and everything in between
                        </h2>
                        <div class="sb-searchbox__outer container py-3 rounded-5 my-3" data-sb-outer" style="background-color: #fff; border: 1px solid #eee; border-radius: 5px;">
                            <form class="row g-3 px-2 gap-2 position-relative" action="/BookHotel" method="post">

                                <div class="col-3 mx-0 px-0">
                                    <label for="location" class="mb-0" style="color: #333;">Location</label>
                                    <div class="search-input ">
                                        <a href="" target="_blank" hidden></a>
                                        <input class="searchInp" type="text" placeholder="Type to search..">
                                        <div class="autocom-box position-absolute shadow rounded" style="min-width:256px; background:white;">
                                            <!-- here list are inserted from javascript -->
                                        </div>
                                    </div>

                                </div>
                                <div class="col-2 mx-0 px-0">
                                    <label for="arrivalTime" class="mb-0" style="color: #333;">Arrival Time</label>
                                    <input type="date" min="" id="arrivalTime" name="arrivalTime" class="arrivalTime form-control bg-orange-100 text-dark" required="" style="border: 1px solid #eee; border-radius: 5px;">
                                </div>
                                <div class="col-2 mx-0 px-0">
                                    <label for="departureTime" class="mb-0" style="color: #333;">Departure Time</label>
                                    <input type="date" id="departureTime" name="departureTime" class="form-control arrivalTime bg-orange-100 text-dark" required="" style="border: 1px solid #eee; border-radius: 5px;">
                                </div>
                                <script>
                                    const today = new Date().toISOString().slice(0, 10); // Get today's date in YYYY-MM-DD format
                                    document.getElementsByClassName('arrivalTime').min = today; // Set the input's min attribute to today's date
                                </script>
                                <div class="col-3 mx-0 px-0">
                                    <label for="location" class="mb-0" style="color: #333;">Persons</label>
                                    <div class="search-input ">
                                        <a href="" target="_blank" hidden></a>
                                        <input class="searchInp" type="text" placeholder="Type to search..">
                                        <div class="autocom-box position-absolute shadow rounded" style="min-width:256px; background:white;">
                                            <!-- here list are inserted from javascript -->
                                        </div>
                                    </div>

                                </div>
                                <div class="col-auto mx-0 px-0 flex-grow-1 ">
                                    <div class="row h-100 w-100 p-0 m-0 justify-content-center align-content-end">
                                        <div class="col-auto p-0 m-0 w-100 h-75">
                                            <button type="submit" class=" btn btn-primary w-100 h-100" style="background-color: #007a5a; border: 1px solid #007a5a; border-radius: 5px;">Search</button>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>


                    </div>
                </div>
            </div>
        </div>
    </div>
 

    
